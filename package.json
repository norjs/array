{
  "name": "@norjs/array",
  "version": "1.3.2",
  "description": "Optimized Array Utility Library",
  "main": "src/index.js",
  "scripts": {
    "compile-watch": "babel --watch --presets es2015 -d dist/ src/",
    "compile": "babel --presets es2015 -d dist/ src/",
    "prepare": "npm run -s compile",
    "pretest": "npm run -s compile",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "test": "npm run test-spec",
    "test-dot": "mocha -u bdd -c -R dot tests/test-*.js",
    "test-list": "mocha -u bdd -c -R list tests/test-*.js",
    "test-spec": "mocha -u bdd -c -R spec tests/test-*.js",
    "test-progress": "mocha -u bdd -c -R progress tests/test-*.js",
    "test-min": "mocha -u bdd -c -R min tests/test-*.js",
    "test-markdown": "mocha -u bdd -c -R markdown tests/test-*.js",
    "lint": "node-lint src/ --config=lint.json",
    "jshint": "jshint src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norjs/array.git"
  },
  "keywords": [
    "array",
    "forEach",
    "map",
    "filter"
  ],
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/norjs/array/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "lint": "^1.1.2",
    "jshint": "^2.9.2",
    "benchmark": "^2.1.1",
    "microtime": "^2.1.1"
  },
  "homepage": "https://github.com/norjs/array"
}
